<?xml version="1.0"?>
<Container version="2">
  <Name>documentserver</Name>
  <Repository>onlyoffice/documentserver</Repository>
  <Registry>https://hub.docker.com/r/onlyoffice/documentserver/</Registry>
  <Network>onlyoffice</Network>
  <MyIP/>
  <Shell>bash</Shell>
  <Privileged>false</Privileged>
  <Support>support-thread.com</Support>
  <Project>https://www.onlyoffice.com/</Project>
  <Overview>ONLYOFFICE Document Server is an online office suite comprising viewers and editors for texts, spreadsheets and presentations, fully compatible with Office Open XML formats: .docx, .xlsx, .pptx and enabling collaborative editing in real time.&#xD;
	HTTPS Support can be enable creating a "certs" folder in the data directory and placing onlyoffice.crt and onlyoffice.key in it. Add fonts by placing .ttf in the font folder and force updating the container.</Overview>
  <Category>Productivity: Tools:</Category>
  <WebUI>http://[IP]:[PORT:8086]</WebUI>
  <TemplateURL>https://raw.githubusercontent.com/AMJidovu/unraid-repository/master/docker-templates/documentserver.xml</TemplateURL>
  <Icon>https://raw.githubusercontent.com/AMJidovu/unraid-repository/d5a562fa4a7b73065dd022e585d80d4b77d0abfa/img/onlyoffice-ce-ds.jpg</Icon>
  <ExtraParams/>
  <PostArgs/>
  <CPUset/>
  <DateInstalled>1575145586</DateInstalled>
  <DonateText>Donate me a coffee or a beer if you like my work :)</DonateText>
  <DonateLink>https://www.paypal.me/xternet</DonateLink>
  <Description>ONLYOFFICE Document Server is an online office suite comprising viewers and editors for texts, spreadsheets and presentations, fully compatible with Office Open XML formats: .docx, .xlsx, .pptx and enabling collaborative editing in real time.&#xD;
	HTTPS Support can be enable creating a "certs" folder in the data directory and placing onlyoffice.crt and onlyoffice.key in it. Add fonts by placing .ttf in the font folder and force updating the container.</Description>
  <Networking>
    <Mode>onlyoffice</Mode>
    <Publish>
      <Port>
        <HostPort>8086</HostPort>
        <ContainerPort>80</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>4432</HostPort>
        <ContainerPort>443</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/user/appdata/onlyoffice/DocumentServer/logs</HostDir>
      <ContainerDir>/var/log/onlyoffice </ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/user/appdata/onlyoffice/DocumentServer/data</HostDir>
      <ContainerDir> /var/www/onlyoffice/Data</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/user/appdata/onlyoffice/DocumentServer/lib</HostDir>
      <ContainerDir>/var/lib/onlyoffice</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/user/appdata/onlyoffice/DocumentServer/db</HostDir>
      <ContainerDir>/var/lib/postgresql</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/user/appdata/onlyoffice/DocumentServer/fonts</HostDir>
      <ContainerDir>/usr/share/fonts/</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Environment/>
  <Labels/>
  <Config Name="Host Port 1" Target="80" Default="8086" Mode="tcp" Description="Container Port: 80" Type="Port" Display="always" Required="true" Mask="false">8086</Config>
  <Config Name="Host Port 2" Target="443" Default="4432" Mode="tcp" Description="Container Port : 443" Type="Port" Display="always" Required="true" Mask="false">4432</Config>
  <Config Name="Host Path 2" Target="/var/log/onlyoffice " Default="/mnt/user/appdata/onlyoffice/DocumentServer/logs" Mode="rw" Description="Container Path: /var/log/onlyoffice " Type="Path" Display="always" Required="true" Mask="false">/mnt/user/appdata/onlyoffice/DocumentServer/logs</Config>
  <Config Name="Host Path 3" Target=" /var/www/onlyoffice/Data" Default="/mnt/user/appdata/onlyoffice/DocumentServer/data" Mode="rw" Description="Container Path:  /var/www/onlyoffice/Data" Type="Path" Display="always" Required="true" Mask="false">/mnt/user/appdata/onlyoffice/DocumentServer/data</Config>
  <Config Name="Host Path 4" Target="/var/lib/onlyoffice" Default="/mnt/user/appdata/onlyoffice/DocumentServer/lib" Mode="rw" Description="Container Path: /var/lib/onlyoffice" Type="Path" Display="always" Required="true" Mask="false">/mnt/user/appdata/onlyoffice/DocumentServer/lib</Config>
  <Config Name="Host Path 5" Target="/var/lib/postgresql" Default="/mnt/user/appdata/onlyoffice/DocumentServer/db" Mode="rw" Description="Container Path: /var/lib/postgresql" Type="Path" Display="always" Required="true" Mask="false">/mnt/user/appdata/onlyoffice/DocumentServer/db</Config>
  <Config Name="Host Path 6" Target="/usr/share/fonts/" Default="/mnt/user/appdata/onlyoffice/DocumentServer/fonts" Mode="rw" Description="Container Path: /usr/share/fonts/" Type="Path" Display="always" Required="true" Mask="false">/mnt/user/appdata/onlyoffice/DocumentServer/fonts</Config>
</Container>
